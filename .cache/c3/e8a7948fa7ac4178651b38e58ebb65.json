eight: 900;\r\n    outline: none;\r\n\r\n    &:hover {\r\n        cursor: pointer;\r\n    }\r\n}\r\n\r\nimg {\r\n    max-width: 100%;\r\n    vertical-align: top;\r\n    border-style: none;\r\n}\r\n\r\n\r\n/******************************************/\r\n\r\n#nav-bar {\r\n    * {\r\n        font-family: $godic-font;\r\n    }\r\n    #nav-logo {\r\n        position: fixed;\r\n        top: 30px;\r\n        left: 30px;\r\n        width: 40px;\r\n        height: 40px;\r\n        background-size: 25px 25px;\r\n        background-repeat: no-repeat;\r\n        background-image: url(\"../img/subLogo.svg\");\r\n        object-fit: cover;\r\n    }\r\n    .btn {\r\n        top: 35px;\r\n        height: $button-height;\r\n        width: $button-width;\r\n    }\r\n    .go.login.btn {\r\n        position: fixed;\r\n        right: 150px;\r\n        cursor: pointer;\r\n    }\r\n    .go.fee-page.btn {\r\n        background-color: $purple;\r\n        color: $yellow;\r\n        position: fixed;\r\n        right: 30px;\r\n        width: $button-width + 35px;\r\n        background-repeat: no-repeat;\r\n        cursor: pointer;\r\n    }\r\n}\r\n\r\n#pages-container {\r\n    max-width: 100%;\r\n    height: auto;\r\n}\r\n\r\n#intro-container {\r\n    padding-left: 50px;\r\n    max-width: 100%;\r\n    height: auto;\r\n    background-color: $yellow;\r\n    .logo {\r\n        width: 80px;\r\n        height: 26px;\r\n        padding-top: 110px;\r\n        background-repeat: no-repeat;\r\n        img {\r\n            height: 100%;\r\n        }\r\n    }\r\n\r\n    .copy-wrap {\r\n        .main-copy {\r\n            padding-top: 40px;\r\n            font-size: 80px;\r\n            font-weight: 900;\r\n        }\r\n        \r\n        .sub-copy {\r\n            margin-top: 20px;\r\n            font-size: $fl;\r\n            strong {\r\n                color: $purple;\r\n            }\r\n        }\r\n    }\r\n    .imgContainer {\r\n        // display: inline;\r\n        top: 50%;\r\n        right: 0;\r\n        width: 70%;\r\n        max-width: 967px;\r\n        text-align: right;\r\n        div {\r\n            display: block;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"177c63ce287d30b86c860f5d4566044c","cacheData":{"env":{}}}